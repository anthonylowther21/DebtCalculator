<?xml version="1.0" encoding="UTF-8"?>
<d:MyBasePage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="DebtCalculator.Pages.SummaryPage"
xmlns:d="clr-namespace:DebtCalculator.Pages">
  <d:MyBasePage.Content>
    <StackLayout Orientation="Vertical" Padding="15">
      <Label Text="Original Interest" ></Label>
      <Label Text="{Binding OriginalInterest}"> </Label>
      <Label Text="Snowball Interest" ></Label>
      <Label Text="{Binding SnowballInterest}"> </Label>
      <Label Text="Saved Interest" ></Label>
      <Label Text="{Binding SavedInterest}"> </Label>
      <Label Text="Original Payoff Date" ></Label>
      <Label Text="{Binding OriginalPayoffDate}"> </Label>
      <Label Text="Snowball Payoff Date" ></Label>
      <Label Text="{Binding SnowballPayoffDate}"> </Label>
    </StackLayout>
  </d:MyBasePage.Content>
</d:MyBasePage> 
